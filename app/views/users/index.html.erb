 <div class="container">
                <div class="row element">
                    	<div class="col-xs-3">

<%= render 'users/list', user: @user, book: @book %>

</div>

<h1>Users</h1>

                        <div class="col-xs-9">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                <th>image</th>
                                <th>name</th>
            	                </tr>
            		        </thead>
            		        <tbody>
            		        	<% @users.each do |user| %>
            		        	<tr>
            		        	<td>
                                <%= attachment_image_tag user, :profile_image, format: 'jpeg', fallback: "no_image.jpg", size: "40x40" %>
                                </td>
                                <td>
                                <%= user.name %>
                                </td>
            		        	<td>
								<%= link_to "Show", user_path(user) %>
            		        	</td>
            		            </tr>
            		        	<% end %>
            		        </tbody>
            	        </table>
                        </div>
                    </div>
                </div>